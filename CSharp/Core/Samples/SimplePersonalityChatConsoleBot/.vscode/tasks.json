{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build (Debug)",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Microsoft.Bot.Builder.PersonalityChat.Core.Sample.Basic.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "Build (Release)",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Microsoft.Bot.Builder.PersonalityChat.Core.Sample.Basic.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        }
    ]
}